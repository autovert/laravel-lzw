{
    "name": "openbitapp/laravel-lzw",
    "description": "LZW Compressor/Decompressor for Laravel",
    "keywords": [
        "bitapp",
        "laravel",
        "compression",
        "lzw"
    ],
    "homepage": "https://github.com/openbitapp/laravel-lzw",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Martini",
            "email": "andrea.martini@bitapp.it",
            "homepage": "https://bitapp.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Openbitapp\\LZW\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Openbitapp\\LZW\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Openbitapp\\LZW\\LZWProvider"
            ],
            "aliases": {
                "LZW": "Openbitapp\\LZW\\LZWFacade"
            }
        }
    }
}
